<% if @products.any? %>
  <% @products.each do |product| %>
    <ul class= "list-unstyled">
      <li id= "cornhole">
        <%= link_to image_tag(product.photo.url(:thumb)), product_path(product) %>
        <%= link_to product.name, product_path(product) %>
        $<%= product.price %>
      </li>
    </ul>
  <% end %>
<% end %>
